
public class FindSubstring {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "456";
		String t = "1235678910111213";
		
		
	}

	public FindSubstring () {
		
	}
		
	
		

	
	class RoolingHash {

		private String txt;
		private double hashValue;
		
		public RoolingHash(String txt) {
			this.txt=txt;
			this.calculateHash();
		}
		
		
		
		private void calculateHash() {			
			for (int i=0; i<this.txt.length(); i++) {
				char iChar = this.txt.charAt(i);
				int numericValue = iChar.
			}
		}



		public void append(char c) {
			this.txt+=c;
			calculateHash();
		}
		
		public void pop(){
			this.txt = this.txt.substring(1);
		}
		
		public double rollingHash() {
			return hashValue;
		}
	}	
	
}



